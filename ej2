# Clase base
class Animal:
    def hacerSonido(self):
        raise NotImplementedError("Este m√©todo debe ser implementado por una subclase")

# Clase derivada: Perro
class Perro(Animal):
    def hacerSonido(self):
        return "Guau"

# Clase derivada: Gato
class Gato(Animal):
    def hacerSonido(self):
        return "Miau"

# Lista de animales y uso del polimorfismo
animales = [Perro(), Gato(), Gato(), Perro()]

for animal in animales:
    print(animal.hacerSonido())
